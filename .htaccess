<IfModule mod_rewrite.c>
    Options -Indexes
    RewriteEngine On
    RewriteBase /yvu_admin_live/
    
    <Files "index.php">
        Require all granted
    </Files>
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ index.php/$1 [L]
    
    # RewriteEngine On
    # RewriteRule ^test$ test.html [L]
</IfModule>
